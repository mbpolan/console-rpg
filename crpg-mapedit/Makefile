####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: crpg-mapedit.pro  /usr/lib/qt3/mkspecs/default/qmake.conf /usr/lib/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile crpg-mapedit.pro
qmake: 
	@$(QMAKE) -o Makefile crpg-mapedit.pro

dist: 
	@mkdir -p .tmp/crpg-mapedit && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/crpg-mapedit/ && ( cd `dirname .tmp/crpg-mapedit` && $(TAR) crpg-mapedit.tar crpg-mapedit && $(GZIP) crpg-mapedit.tar ) && $(MOVE) `dirname .tmp/crpg-mapedit`/crpg-mapedit.tar.gz . && $(DEL_FILE) -r .tmp/crpg-mapedit

